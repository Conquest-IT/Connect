{
    "name": "shelai-dholai-backend",
    "version": "1.0.0",
    "description": "This is the back end of the Shelai Dholai Mobile Application",
    "main": "server/server.js",
    "scripts": {
        "build": "webpack --mode production",
        "start:webpack": "webpack-dev-server --mode development",
        "start": "node server/server.js",
        "dev": "NODE_ENV=test nodemon server/server.js",
        "doc": "jsdoc -c jsdoc.json",
        "both": "concurrently \"npm run dev\" \"npm run start:webpack\"",
        "prisma:migrate": "cd server && npx prisma migrate dev --name init",
        "prisma:generate": "cd server && npx prisma generate",
        "prisma:studio": "cd server && npx prisma studio",
        "heroku-postbuild": "npm run prisma:generate"
    },
    "dependencies": {
        "@emotion/react": "^11.10.4",
        "@emotion/styled": "^11.10.4",
        "@fontsource/roboto": "^4.5.8",
        "@mui/icons-material": "^5.10.9",
        "@mui/material": "^5.10.9",
        "@mui/styled-engine-sc": "^5.10.6",
        "@prisma/client": "^4.3.1",
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.0",
        "cloudinary": "^1.32.0",
        "cloudinary-react": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "helmet": "^6.0.0",
        "js-cookie": "^3.0.1",
        "jsdoc": "^3.6.11",
        "jsonwebtoken": "^8.5.1",
        "mui-tel-input": "^2.0.3",
        "otp-generator": "^4.0.0",
        "prisma": "^4.3.1",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.4.0",
        "react-router-dom": "^5.2.0",
        "recharts": "^2.1.15",
        "styled-components": "^5.3.6",
        "twilio": "^3.82.0"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-react": "^7.18.6",
        "babel-loader": "^8.2.5",
        "concurrently": "^7.4.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.23.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "image-webpack-loader": "^8.1.0",
        "nodemon": "^2.0.19",
        "sass": "^1.55.0",
        "sass-loader": "^13.0.2",
        "style-loader": "^3.3.1",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1"
    }
}
